@model IEnumerable<Service.DTO.TransactionDTO>
<div class="row">
    <div class="col-sm-12">
        <!-- Autofill table start -->
        <div class="card">
            <div class="card-header">
                <h5>Auto Fill</h5>
                <span>AutoFill gives an Excel like option to a DataTable to click and drag over multiple cells, filling in information over the selected cells and incrementing numbers as needed.</span>
                <div class="card-header-right">
                    <ul class="list-unstyled card-option">
                        <li><i class="feather icon-maximize full-card"></i></li>
                        <li><i class="feather icon-minus minimize-card"></i></li>
                        <li><i class="feather icon-trash-2 close-card"></i></li>
                    </ul>
                </div>
            </div>
            <div class="card-block">
                <div class="dt-responsive table-responsive">
                    <table id="listTrade" class="table table-striped table-bordered nowrap">
                        <thead>
                            <tr>
                                <th>@Html.DisplayNameFor(model => model.Id)</th>
                                <th>@Html.DisplayNameFor(model => model.Product.Name)</th>
                                <th>@Html.DisplayNameFor(model => model.StartDate)</th>
                                <th>@Html.DisplayNameFor(model => model.EndDate)</th>
                                <th>@Html.DisplayNameFor(model => model.StartPrice)</th>
                                <th>@Html.DisplayNameFor(model => model.EndPrice)</th>
                                <th>@Html.DisplayNameFor(model => model.Quantity)</th>
                                <th>@Html.DisplayNameFor(model => model.PnL)</th>
                                <th>@Html.DisplayNameFor(model => model.Statuts)</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Id)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Product.Name)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.StartDate)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.EndDate)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.StartPrice)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.EndPrice)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Quantity)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.PnL)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Statuts)
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
